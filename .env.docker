# Docker environment variables for single container deployment  
FLASK_ENV=production
DATABASE_URL=sqlite:///./data/myriade.db
STATIC_FOLDER=./static
HOST=http://localhost:8080

# Default values for required variables
GUNICORN_THREADS=4
AGENTLYS_PROVIDER=proxy
AGENTLYS_MODEL=claude-sonnet-4-5-20250929
AGENTLYS_OUTPUT_SIZE_LIMIT=20000
LOG_LEVEL=INFO

# These should be set at runtime via docker run -e
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
